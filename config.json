{
    "watch_path": "./watch_target",
    "patterns": {
        "filenames": ["report.txt", "important.csv"],
        "extensions": [".log", ".csv"],
        "prefixes": ["backup_", "temp_"],
        "suffixes": ["_final", "_completed"],
        "regex": ["^data_\\d{4}-\\d{2}-\\d{2}\\.csv$", "^log_.*\\.txt$"]
    }
}
